/* 公共样式 */
page{
	padding-bottom: 112rpx;
}
page,view{
	box-sizing:border-box;
	-moz-box-sizing:border-box; /* Firefox */
	-webkit-box-sizing:border-box; /* Safari */
}
image{
	display: block;
}
/* 自定义空白占位行 */
.h_20{
	height: 20rpx;
}
/* 公用样式 */
.no_br{
	border-right: 0 !important;
}
